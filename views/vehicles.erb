<ul>
  <% if Vehicle.length() == 0 %>
    There are no vehicles on the lot at this time : (
  <% else %>
    <% @vehicles.each() do |vehicle| %>
      <li><a href="/vehicles/<%= vehicle.id() %>"><%= vehicle.make() %>, <%= vehicle.model() %>, <%= vehicle.year() %></a></li>
    <% end %>
  <% end %>
</ul>
